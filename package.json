    "@material-ui/styles": "^4.11.4",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "concurrently": "^6.2.0",
    "dotenv": "^10.0.0",
    "jest-image-snapshot": "^4.5.0",
    "lodash": "^4.17.21",
    "puppeteer": "^9.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-github-corner": "^2.5.0",
    "react-image": "^4.0.3",
    "react-pdf": "^5.3.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^17.0.2",
    "react-use": "^17.2.4",
    "sass": "^1.35.1",
    "source-map-explorer": "^2.5.2",
    "styled-components": "^5.3.0",
    "typeface-bangers": "^1.1.13",
    "typeface-bebas-neue": "^1.1.13",
    "typeface-courier-prime": "^1.1.13",
    "typescript": "^4.2.4",
    "web-vitals": "^1.1.2",
    "workbox-build": "^6.1.5",
    "workbox-precaching": "^6.1.5",
    "workbox-routing": "^6.1.5",
    "workbox-strategies": "^6.1.5"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "analyze-bundle": "npm run build && npm run analyze",
    "start": "react-scripts start",
    "start-developing": "concurrently \"npm start\" \"npm run storybook\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-coverage": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@types/autosuggest-highlight": "^3.1.1",
    "@types/clientjs": "^0.1.0",
    "@types/jest": "^26.0.23",
    "@types/jest-image-snapshot": "^4.3.0",
    "@types/lodash": "^4.14.170",
    "@types/react-pdf": "^5.0.4",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.9",
    "eslint": "^7.28.0"
  }
}
